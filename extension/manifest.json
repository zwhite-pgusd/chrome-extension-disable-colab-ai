{
  "manifest_version": 3,
  "name": "Colab AI Off",
  "version": "1.0.0",
  "description": "Chrome extension to disable AI features in Google Colab.",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://colab.research.google.com/*"],
      "css": ["content.css"],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "update_url": "https://zwhite-pgusd.github.io/chrome-extension-disable-colab-ai/docs/updates.xml",
  "key": ""
}
